<div data-role="header">
    <h1><%- party.get("name") %></h1>
    <a href="#partyoverview" data-rel="back" data-role="button" data-icon="arrow-l">back</a>
    <a href="#activeparty_<%- party.id %>_addsong" data-transition="slidedown" data-role="button">Add song</a>
</div>

<div data-role="content">
    <ul class="tracks" data-role="listview">
        <li class="playlist-empty-message">
            <div>The playlist is empty, you can add songs with the button in the right top</div>
        </li>
        <li data-role="list-divider" id="playlist-divider">Playlist</li> 
    </ul>
</div>

